			"name": "Create Booking TCs",
			"item": [
				{
					"name": "TC#1 Create Booking Positive TC",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"//Test 1: Validate Response code\r",
									"pm.test(\"Response status code is 200 or 201\", function () {\r",
									"    pm.expect(pm.response.code).to.be.oneOf([200,201]);\r",
									"});\r",
									"\r",
									"//Test 2: Validate Booking ID in Response Body\r",
									"pm.test(\"Response body contains booking id\", function(){\r",
									"    var responseData = pm.response.json();\r",
									"    pm.expect(responseData).to.have.property(\"bookingid\");\r",
									"});\r",
									"\r",
									"//Test 3: Validate Booking Details in Response Body\r",
									"\r",
									"pm.test(\"Response body contains booking details\", function () {\r",
									"    var responseData = pm.response.json();\r",
									"    pm.expect(responseData).to.have.property(\"booking\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"firstname\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"lastname\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"totalprice\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"depositpaid\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"bookingdates\");\r",
									"    pm.expect(responseData.booking.bookingdates).to.have.property(\"checkin\");\r",
									"    pm.expect(responseData.booking.bookingdates).to.have.property(\"checkout\");\r",
									"    pm.expect(responseData.booking).to.have.property(\"additionalneeds\");\r",
									"});\r",
									"\r",
									"//Test 4: Validate first name in the response body\r",
									"pm.test(\"Response body contains correct first name and Last name\", function () {\r",
									"    var responseData=pm.response.json();\r",
									"    pm.expect(responseData.booking.firstname).to.eql(\"Jim\");\r",
									"    pm.expect(responseData.booking.lastname).to.eql(\"Brown\");\r",
									"});\r",
									"\r",
									"//Test 5: Validate total price in the response body\r",
									"\r",
									"pm.test(\"Response body contains correct total price and deposite paid\", function () {\r",
									"    var responseData=pm.response.json();\r",
									"    pm.expect(responseData.booking.totalprice).to.eql(111);\r",
									"    pm.expect(responseData.booking.depositpaid).to.eql(true);\r",
									"});\r",
									"\r",
									"pm.test(\"Response body contains correct checkin and checkoutn dates\", function () {\r",
									"    var responseData=pm.response.json();\r",
									"    pm.expect(responseData.booking.bookingdates.checkin).to.eql(\"2018-01-01\");\r",
									"    pm.expect(responseData.booking.bookingdates.checkout).to.eql(\"2019-01-01\");\r",
									"});\r",
									"\r",
									"console.log(pm.response.json());\r",
									"\r",
									"//Test 6: Validate additional needs in the response body\r",
									"pm.test(\"Additional needs is 'Breakfast'\", function () {\r",
									"    var responseData = pm.response.json();\r",
									"    pm.expect(responseData.booking.additionalneeds).to.eql(\"Breakfast\");\r",
									"});\r",
									"\r",
									"\r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
